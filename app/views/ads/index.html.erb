<p id="notice"><%= notice %></p>

<h1>Ads</h1>

<table>
  <thead>
    <tr>
      <th>Reference</th>
      <th>Status</th>
      <th>Description</th>
    </tr>
  </thead>

  <tbody>
    <% @ads.each do |ad| %>
      <tr>
        <td><%= ad.reference %></td>
        <td><%= ad.status %></td>
        <td><%= ad.description %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
